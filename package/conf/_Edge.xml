<?xml version="1.0" encoding="UTF-8"?>

<Server version="1">
	<Name>OvenMediaEngine</Name>
	<Hosts>
		<Host>
			<Name>default</Name>
			<!-- TODO: NEED TO CHANGE THIS IP ADDRESS -->
			<IP>@LOCAL_IP</IP>
			<Applications>
				<Application>
					<Name>app</Name>
					<Type>liveedge</Type>
					<Origin>
						<Primary>@ORIGIN_IP:9000</Primary>
						<Secondary>@ORIGIN_IP:9000</Secondary>
